#!/usr/bin/stap

probe begin {
    printf("Practica 1\n");
}

probe syscall.read {
    if(pid() == $1 || pid() == $2){
        printf("PID[%d] %s -> Read\n", pid(), execname());
    }
}

probe syscall.write {
    if(pid() == $1 || pid() == $2){
        printf("PID[%d] %s -> Write\n", pid(), execname());
    }
}